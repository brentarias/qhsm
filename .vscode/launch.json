{
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Launch (console)",
			"type": "dotnet",
			"request": "launch",
			"preLaunchTask": "build",
			"projectPath": "${workspaceFolder}/TestProgram/TestProgram.csproj"
		},
		{
			"name": ".NET Attach",
			"type": "dotnet",
